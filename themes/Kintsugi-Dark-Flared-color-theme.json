  "tokenColors": [
    {
      "name": "Default Text, Variables, Properties",
      "scope": ["text", "source", "variable"],
      "settings": {
        "foreground": "#BCAC8F"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.comment"],
      "settings": {
        "foreground": "#5f5f5f"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "delimiter",
        "bracket",
        "brace",
        "paren",
        "delimiter.tag",
        "punctuation.tag",
        "tag.html",
        "tag.xml",
        "meta.property-value punctuation.separator.key-value",
        "punctuation.definition.metadata.md",
        "string.link.md",
        "meta.brace",
        "punctuation.definition.string",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#7f7b66"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "meta.property-value.keyword",
        "support.constant.property-value.keyword",
        "meta.preprocessor.keyword",
        "keyword.other.use",
        "keyword.other.function.use",
        "keyword.other.namespace",
        "keyword.other.new",
        "keyword.other.special-method",
        "keyword.other.unit",
        "keyword.other.use-as"
      ],
      "settings": {
        "foreground": "#D66848",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage & Bolded Items",
      "scope": [
        "storage",
        "entity.name.tag",
        "keyword.operator.new",
        "variable.language.this",
        "variable.language.self",
        "meta.qualified_type.cpp entity.name.type.cpp"
      ],
      "settings": {
        "foreground": "#DBAD49",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Language-Specific Constructs",
      "scope": [
        "meta.definition.variable.java storage.type.java",
        "meta.catch.parameters.java storage.type.java",
        "meta.import.java storage.modifier.import.java",
        "meta.function-call.python meta.function-call.generic.python",
        "meta.function.python support.variable.magic.python "
      ],
      "settings": {
        "foreground": "#798283"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#E08542",
        "fontStyle": ""
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "meta.property-value.string",
        "support.constant.property-value.string",
        "meta.structure.dictionary.value.json string.quoted.double.json",
        "meta.structure.dictionary.json string.quoted.double.json",
        "meta.preprocessor string",
        "string.template"
      ],
      "settings": {
        "foreground": "#cc7f66"
      }
    },
    {
      "name": "String Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#798283"
      }
    },
    {
      "name": "Primitive Literals (Numbers, Booleans, etc)",
      "scope": [
        "constant.numeric",
        "meta.property-value.numeric",
        "support.constant.property-value.numeric",
        "meta.property-value.color",
        "support.constant.property-value.color",
        "constant.language"
      ],
      "settings": {
        "foreground": "#DB9833"
      }
    },
    {
      "name": "User-defined names (Functions, Classes, etc)",
      "scope": [
        "constant.character",
        "constant.other",
        "entity.name.function",
        "entity.name.class",
        "entity.other.inherited-class",
        "entity.other.attribute-name",
        "entity.name",
        "entity.name.type.class",
        "entity.other.attribute-name.html",
        "support.type.property-name",
        "entity.name.tag.table",
        "meta.structure.dictionary.json string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#798283"
      }
    },

    {
      "name": "Un-bold specific storage types",
      "scope": [
        "storage.type.annotation.java",
        "storage.type.java",
        "storage.type.object.array.java",
        "entity.name.type",
        "meta.qualified_type.cpp entity.name.type.cpp",
        "meta.import.java storage.modifier.import.java"
      ],
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Pointer, access, etc",
      "scope": ["meta.ptr", "meta.pointer", "meta.address", "meta.array.cxx"],
      "settings": {
        "foreground": "#EBA96C"
      }
    },
    {
      "name": "Preprocessor",
      "scope": ["meta.preprocessor"],
      "settings": {
        "foreground": "#798283"
      }
    },
    {
      "name": "Library Support",
      "scope": [
        "support.type",
        "support.class",
        "support.function",
        "support.constant"
      ],
      "settings": {
        "foreground": "#E08542"
      }
    },
    {
      "name": "Generic Types (Containers)",
      "scope": ["storage.type.generic", "entity.name.type.template.cpp"],
      "settings": {
        "foreground": "#E08542",
        "fontStyle": ""
      }
    },
    {
      "name": "Compiler Metadata & Attributes (Annotations, Lifetimes, Decorators)",
      "scope": [
        "storage.type.annotation.java",
        "meta.type.annotation.ts",

        "entity.name.type.lifetime",
        "punctuation.definition.lifetime",

        "meta.attribute.rust",
        "punctuation.definition.attribute.rust",

        "meta.decorator",
        "punctuation.definition.decorator",
        "entity.name.function.decorator",
        "meta.decorator.ts variable.other.readwrite.ts"
      ],
      "settings": {
        "foreground": "#678E87"
      }
    },
    {
      "name": "JSX/TSX Tags",
      "scope": ["entity.name.tag.tsx", "entity.name.tag.jsx"],
      "settings": {
        "foreground": "#DBAD49",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.deprecated"],
      "settings": {
        "foreground": "#b38f8f",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Diff: Header",
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#75715E"
      }
    },
    {
      "name": "Diff: Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#b38f8f"
      }
    },
    {
      "name": "Diff: Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#a3be8c"
      }
    },
    {
      "name": "Diff: Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#ebcb8b"
      }
    },
    {
      "name": "Markdown: Title Hash",
      "scope": [
        "punctuation.definition.heading.md",
        "entity.name.type.md",
        "beginning.punctuation"
      ],
      "settings": {
        "foreground": "#798283"
      }
    },
    {
      "name": "Markdown: Titles & Bold Text",
      "scope": ["markup.heading", "entity.name.section", "markup.bold"],
      "settings": {
        "foreground": "#DBAD49",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown: Raw Code",
      "scope": [
        "markup.raw",
        "markup.inline.raw",
        "markup.fenced",
        "markup.fenced_code"
      ],
      "settings": {
        "foreground": "#cc7f66"
      }
    },
    {
      "name": "Markdown: Link",
      "scope": [
        "markup.link",
        "string.other.link.title",
        "string.other.link.description",
        "meta.link.inline",
        "meta.image.inline"
      ],
      "settings": {
        "foreground": "#798283"
      }
    },
    {
      "name": "Makefile Variables",
